#include <iostream>
using namespace std;

int main() {
    int rows = 5; // Number of rows for the triangle
    int i =1 ; /*        1
                         12
                         123
                         1234
           */              

    while (i <= rows) {
        int j = 1;
        while (j <= i) {
            cout <<j;
            j++;
        }
        cout << endl;
        i++;
    }

    return 0;
}
