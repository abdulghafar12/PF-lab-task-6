#include <iostream>
using namespace std;

int main() {
    int rows = 7; // Number of rows for the triangle
    int i =1;  /*        1
                         22
                         333
                         4444
           */              
    while (i <= rows) {
        int j = 1;
        while (j <= i) {
            cout << i;
            j++;
        }
        cout << endl;
        i++;
    }

    return 0;
}
