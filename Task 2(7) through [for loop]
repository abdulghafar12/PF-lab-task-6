#include <iostream>
using namespace std;

int main() {
    int rows = 5; // Number of rows for the triangle
    int i , j ;  /*   1
                         12
                         123
                         1234
           */              

    for( i= 1 ; i <= rows ; i++)
     {
  for( j = 1 ;  j <= i ;  j++) 
  {
            cout <<j;
        }
        cout << endl;
     }
    return 0;
}
