#include <iostream>
using namespace std;

int main() {
    int rows = 7; // Number of rows for the triangle
    int i , j ;  /*      1
                         22
                         333
                         4444
           */              
    for ( i = 1 ;  i <= rows ;  i++) 
    {
        for (j = 1 ; j <= i ; j++)
         {
            cout << i;
        }
        cout << endl;
    }

    return 0;
}
